<div class="page w-full mx-auto my-10 hidden" id="educationalDetails">

  <div class="w-11/12 mx-auto">
    <h3 class="underline font-semibold text-2xl">Educational Details</h3>
    <div class="w-[90%] flex justify-between my-3 gap-10">
      <div class="w-[33%] flex justify-between">
        <label>Degree Name</label>
        <select class="w-[218px]">
          <option>Select Any Degree</option>
          <option>SSC</option>
          <option>HSC</option>
          <option>Diploma</option>
          <option>Bachelor</option>
          <option>Master</option>
        </select>      
      </div>
    </div>

    <div class="w-[90%] flex justify-between my-3 gap-10">
      <div class="w-[33%] flex justify-between">
        <label>Name of Board</label>
        <input type="text" name="board[]" class="border-2" placeholder="Enter board Name">
      </div>
      <div class="w-[28%] flex justify-between">
        <label>Passing Year</label>
        <input type="text" name="year[]" class="border-2" placeholder="Enter Passing Year">
      </div>
      <div class="w-[28%] flex justify-between">
        <label>Percentage</label>
        <input type="text" name="percentage[]" class="border-2" placeholder="Enter Percentage">
      </div>
    </div>

  </div>
</div>